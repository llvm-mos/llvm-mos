    %1:any(s8) = G_CONSTANT i8 -1
    %2:any(p0) = G_PTR_ADD %0, %1
    %3:any(s8) = G_LOAD %2 :: (load 1)
    RTS implicit %3
# CHECK:      %0:imag16 = IMPLICIT_DEF
# CHECK-NEXT: %1:yc = LDimm -1
# CHECK-NEXT: %3:ac = LDyindir %0, %1
# CHECK-NEXT: RTS
...

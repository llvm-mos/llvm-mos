name: load
# CHECK-LABEL: name: load
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
    %0:imag16(p0) = G_IMPLICIT_DEF
    %1:any(s8) = G_LOAD %0:imag16(p0) :: (load 1)
    RTS implicit %1
# CHECK:      %2:yc = LDimm 0
# CHECK-NEXT: %1:ac = LDyindir %0, %2
# CHECK-NEXT: RTS
...
---
name: load_indexed_indir
# CHECK-LABEL: name: load
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
